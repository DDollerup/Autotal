@model ProductVM

<div class="row">
    <div id="mostExpensiveCar">
        <div>
            <div>
                <span>@Model.Product.Model (@Model.Product.Views visninger)</span>
            </div>
            <div>
                <img src="~/Content/Images/Products/@Model.GetFirstImage().ImageURL" alt="@Model.GetFirstImage().Alt" class="img-responsive" />
                <span>
                    @Model.Product.Price
                </span>
            </div>
        </div>
    </div>
</div>
